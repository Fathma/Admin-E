<div class="row">
    <div class="col-md-offset-2 col-md-8 col-lg-offset-3 col-lg-6">
        <div class="well profile">
            <div class="col-sm-12 ">
                <h3>Posts </h3>
                {{# if posts}}
                <table class="table">
                    <th>Title</th>
                    <th>Text</th>
                    <th>Date</th>
                    {{# each posts }}
                    <tr>
                        <td>{{this.name}}</td>
                        <td>{{this.text}}</td>
                        <td>{{formatDate this.createDate}}</td>
                    </tr>
                    {{/each}}
                </table>
                {{else}}
                No posts
                {{/if}}
            </div>
        </div>
    </div>
</div>